{
  "name": "develop",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "vite packages/licoimg",
    "build": "vite build packages/licoimg",
    "preview": "vite preview packages/licoimg",
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx",
    "lint:fix": "eslint . --ext .js,.ts,.jsx,.tsx --fix",
    "format": "prettier --write .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "commit": "cz",
    "prepare": "husky",
    "gh": "gh"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css,html}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/js": "^9.17.0",
    "@types/node": "^22.9.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.17.0",
    "gh": "^2.8.9",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "^3.6.2",
    "typescript": "^5.7.2",
    "vite": "^7.2.4",
    "vitest": "^2.1.5",
    "yarn": "^1.22.22"
  }
}